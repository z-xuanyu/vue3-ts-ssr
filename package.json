    "koa-connect": "^2.1.0",
    "koa-send": "^5.0.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.2",
    "vue-tsc": "^0.29.8"
  }
}
